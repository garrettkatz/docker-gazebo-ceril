<launch>
  <arg name="robot" default="poppy"/>
  <arg name="publish_frequency" value="500" />
  <!--Robot pose -->
  <arg name="x" default="0"/>
  <arg name="y" default="0"/>
  <arg name="z" default="0.4"/>

  <arg name="roll" default="1.57"/>
  <arg name="pitch" default="0"/>
  <arg name="yaw" default="0"/>

  <arg name="initial_y" default="0"/>
  <arg name="initial_r" default="0"/>
  <arg name="initial_p" default="0"/>
  <arg name="initial" default="0"/>

<!-- Used to have pids setup here, dont think we need it-->

  <!-- setup yaml values before model loading -->
  <include file="$(find poppy_gazebo)/launch/poppy_yaml_setup.launch">
  </include>

  <!--
  <param name="robot_description"
    textfile="$(find poppy_description)/urdf/Poppy_Humanoid.URDF" />


  <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
    args="-file $(find poppy_description)/urdf/Poppy_Humanoid.URDF -urdf -model poppy -x $(arg x) -y $(arg y) -z $(arg z) -R $(arg roll) -P $(arg pitch) -Y $(arg yaw) -unpause " />-->

  <!-- Load the URDF into the ROS Parameter Server -->
  <!--<param name="robot_description"
    textfile="$(find poppy_description)/urdf/Poppy_Humanoid.URDF" />-->

  <param name="robot_description"
    command="$(find xacro)/xacro --inorder '$(find poppy_description)/urdf/poppy.xacro'" />

  <!-- Run a python script to the send a service call to gazebo_ros to spawn a URDF robot -->
  <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
    args="-urdf -model poppy -param robot_description -x $(arg x) -y $(arg y) -z $(arg z) -R $(arg roll) -P $(arg pitch) -Y $(arg yaw) -unpause"/>




  <!-- Load default controllers -->
  <include file="$(find poppy_gazebo)/launch/default_controllers.launch">
    <arg name="publish_frequency" value="$(arg publish_frequency)" />
  </include>


  <!-- Robot state publisher -->
  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher"
    respawn="false" output="screen">
    <param name="publish_frequency" value="$(arg publish_frequency)"/>
    <param name="tf_prefix" value=""/>
    <remap from="joint_states" to="/poppy/joint_states" />
  </node>


  <!-- Motion planning -->
  <!-- <include file="$(find rosbook_arm_moveit_config)/launch/move_group.launch"/> -->

  <!-- create a gui to move joints
  <node name = "joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
    <param name="use_gui" value="true"/>
  </node>
  -->
  <!--show in rviz-->
<!--  <node name="rviz" pkg="rviz" args="-d $(find tiger_gazebo)/launch/config.rviz"/>
-->
</launch>
